{
  "manifest_version": 3,
  "name": "Project Portal",
  "description": "Dashboard for your project's related apps & services.",
  "version": "0.1",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "src/entrypoints/newtab.html"
  },
  "action": {
    "default_popup": "src/entrypoints/popup.html"
  }
}